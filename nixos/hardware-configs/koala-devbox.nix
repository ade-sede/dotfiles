  boot.initrd.luks.devices."luks-3f6a399e-a33c-45da-aeb2-aca142ea37ad".device = "/dev/disk/by-uuid/3f6a399e-a33c-45da-aeb2-aca142ea37ad";

  fileSystems."/boot" = {
    device = "/dev/disk/by-uuid/E8C0-1E75";
    fsType = "vfat";
    options = ["fmask=0077" "dmask=0077"];
